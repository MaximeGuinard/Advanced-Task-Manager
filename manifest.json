{
    "manifest_version": 3,
    "name": "Task Manager Extension",
    "version": "1.1",
    "description": "An extension to manage tasks and improve productivity.",
    "permissions": [
      "storage",
      "alarms",
      "notifications"
    ],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "alarms",
      "notifications"
    ]
  }
  